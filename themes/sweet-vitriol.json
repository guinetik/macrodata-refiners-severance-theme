  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#849aa6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#849aa6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#2d6d62",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#1f5e54",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#b08d57"
      }
    },
    {
      "scope": [
        "string.quoted.double punctuation.definition.string",
        "string.quoted.single punctuation.definition.string",
        "string.template punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#b08d57"
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "string constant.character.escape"
      ],
      "settings": {
        "foreground": "#2a7d78"
      }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#3a8d82"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language.numeric"
      ],
      "settings": {
        "foreground": "#c0a060"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#2d6d62"
      }
    },
    {
