  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9aa8b5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#9aa8b5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#3d6d8f",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#2a4d6b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#508a70"
      }
    },
    {
