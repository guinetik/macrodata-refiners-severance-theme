  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#4a5555",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6a7575",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.control.flow",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.conditional",
        "keyword.control.loop",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#00d5d5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#4a8585",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "string",
        "string.quoted",
        "string.template"
      ],
      "settings": {
        "foreground": "#70b0a0"
